 *  This is an internal header file, included by other library headers.
 *  Do not attempt to use it directly. @headername{locale}
 */__io__fill__news__olds__newlen__oldlen__v__w__new__cs__grouping__grouping_size__sep__err__xtrc__gsize__mod__bufend__lit__dec__grouping_tmp/usr/include/c++/4.8.2/bits/locale_facets.h<bits/locale_facets.tcc><bits/ctype_inline.h><bits/streambuf_iterator.h><bits/ctype_base.h><cwctype>__write__convert_to_v_M_initialize_numpunct~numpunct__numpunct_cacheconst __numpunct_cache<_CharT> &__numpunct_cache<_CharT> &~__numpunct_cache__num_base &const __num_baseconst __num_base &_S_format_floatctype_byname<wchar_t> &const ctype_byname<wchar_t>const ctype_byname<wchar_t> &ctype_byname~ctype_bynamectype_byname<char> &const ctype_byname<char>const ctype_byname<char> &do_narrowdo_widendo_tolowerdo_toupperdo_scan_notdo_scan_isdo_is~__ctype_abstract_base__ctype_abstract_basenarrowwiden~numpunct_bynamenumpunct_bynamedo_falsenamedo_truenamedo_groupingdo_thousands_sepdo_decimal_pointfalsenametruenamenumpunct__cache_type *~num_putputnum_putunsigned long long &long long &unsigned long &unsigned short &_M_findconst _CharT2 *~num_getgetnum_get~ctypectype<wchar_t> &const ctype<wchar_t>const ctype<wchar_t> &_M_initialize_ctype_M_convert_to_wmaskctype<char> &const ctype<char> &_M_widen_init_M_narrow_initclassic_tabletable__builtin_expectchar[256]__dfault__to__cache255static_cast<unsigned char>(-1)1 + static_cast<unsigned char>(-1)__numpunct_cache<_CharT>__num_base_S_iminus_S_iplus_S_ix_S_iX_S_izero_S_ie_S_izero + 14_S_iE20_S_izero + 20_S_iend26_S_ominus_S_oplus_S_ox_S_oX_S_odigits_S_odigits_end_S_odigits + 16_S_oudigits_S_oudigits_end36_S_oudigits + 16_S_oe_S_odigits + 14_S_oE34_S_oudigits + 14_S_oendctype_byname<wchar_t>ctype_byname<char>__ctype_abstract_base<_CharT>__pad<_CharT, _Traits>__cache_typeiter_type__wmask_typeconst size_t_M_allocated_CharT[26]__num_base::_S_iend_M_atoms_in_CharT[36]__num_base::_S_oend_M_atoms_out_M_thousands_sep_M_decimal_point_M_falsename_size_M_falsename_M_truename_size_M_truename_M_use_grouping_M_grouping_size_M_grouping__wmask_type[16]_M_wmaskmask[16]unsigned 